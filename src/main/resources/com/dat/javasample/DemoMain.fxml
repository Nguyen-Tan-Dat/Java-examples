<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="root" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.dat.controller.DemoMainController">
    <left>
        <VBox prefHeight="400.0" prefWidth="170.0" spacing="2.0"
              style="-fx-background-color: #0598ff; -fx-border-color: #9898ff; -fx-border-width: 0 10 0 10;"
              BorderPane.alignment="CENTER">
            <Button mnemonicParsing="false" onMouseClicked="#loadPage1" prefHeight="25.0" prefWidth="177.0"
                    style="-fx-background-color: #0598ff; -fx-border-color: #ffffff; -fx-border-width: 1 0 1 0;"
                    text="Combobox" textFill="WHITE">
                <font>
                    <Font size="15.0"/>
                </font>
            </Button>
            <Button mnemonicParsing="false" onMouseClicked="#loadPage2" prefHeight="25.0" prefWidth="184.0"
                    style="-fx-background-color: #0598ff; -fx-border-color: #ffffff; -fx-border-width: 1 0 1 0;"
                    text="Circle Animation" textFill="WHITE">
                <font>
                    <Font size="15.0"/>
                </font>
            </Button>
        </VBox>
    </left>
</BorderPane>
